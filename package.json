{
  "name": "markup",
  "version": "1.0.0",
  "scripts": {
    "start": "npm-run-all -p sass:* web",
    "sass:watch": "node-sass --output-style expanded src -o src",
    "sass:init": "node-sass --output-style expanded -w src -o src",
    "web": "http-server -p 3000 -g -o --proxy http://127.0.0.1:3000?"
  },
  "devDependencies": {
    "http-server": "^0.12.1",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5"
  }
}
